{
  "name": "chromabro",
  "version": "0.5.0",
  "description": "A simple app that will take webcam input, use tensorflow+body-segmentation to segment the image in two parts (background and person) and place the video in the lower right of screen.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "dist": "electron-builder"
  },
  "keywords": [],
  "author": "Ryan Beales",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3"
  },
  "dependencies": {},
  "build": {
    "appId": "com.ryanbeales.chromabro",
    "productName": "Chromabro",
    "win": {
      "target": "nsis"
    }
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "chromabro"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}