{
  "name": "chromabro",
  "version": "0.5.0",
  "description": "A simple app that will take webcam input, use tensorflow+body-segmentation to segment the image in two parts (background and person) and place the video in the lower right of screen.",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "Ryan Beales",
  "license": "ISC",
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/publisher-github": "^7.11.1",
    "electron": "^40.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "chromabro"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "ryanbeales",
              "name": "chromabro"
            },
            "prerelease": false
          }
        }
      ]
    }
  }
}